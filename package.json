{
  "name": "pelina-beer",
  "version": "2.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx, jsx}": [
      "npx eslint --quiet --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "npx prettier --write ."
    ],
    "*": [
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-all"
    }
  },
  "scripts": {
    "clean": "rm -rf .next;rm ./graphql/queries/*.graphql.d.ts;rm .env.local;rm public/*.js",
    "dev": "NODE_ENV=test next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --check --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --update-snapshot --detectOpenHandles --watchAll",
    "coverage": "watchman watch-del echo $pwd;watchman watch-project echo $pwd;jest --collect-coverage",
    "cypress:open": "cypress open",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "analyzer": "ANALYZE=true NODE_ENV=test next build && yarn start",
    "development": "cp .env.development .env.local && NODE_ENV=development yarn build && yarn start",
    "production": "cp .env.production .env.local && NODE_ENV=production yarn build && yarn start"
  },
  "dependencies": {
    "axios": "0.27.2",
    "gsap": "^3.10.4",
    "imagesloaded": "^5.0.0",
    "locomotive-scroll": "^4.1.4",
    "next": "12.1.6",
    "next-auth": "4.10.3",
    "parcel": "^2.6.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "styled-components": "5.3.5",
    "swiper": "^8.1.6",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "7.17.12",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@next/bundle-analyzer": "12.1.6",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.186",
    "@swc/jest": "0.2.21",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@types/jest": "27.5.1",
    "@types/mocha": "9.1.1",
    "@types/next-auth": "3.15.0",
    "@types/node": "17.0.35",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-test-renderer": "18.0.0",
    "@types/styled-components": "5.1.25",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "@zeit/next-css": "1.0.1",
    "css-loader": "6.7.1",
    "cypress": "9.6.1",
    "eslint": "8.15.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "12.4.1",
    "next-pwa": "5.5.2",
    "next-sitemap": "2.5.20",
    "next-transpile-modules": "9.0.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "sass": "1.51.0",
    "sharp": "0.30.4",
    "swc-loader": "0.2.1",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  }
}
